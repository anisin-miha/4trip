  "scripts": {
    "dev": "next dev",
    "dev:trip": "NEXT_PUBLIC_SITE=trip SITE=trip next dev -p 3000",
    "dev:bus": "next dev apps/bus -p 3001",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d out",
    "i18n:extract": "tsx scripts/i18n-extract.ts",
    "i18n:gen-pages": "tsx scripts/i18n-gen-pages.ts",
    "gen": "yarn i18n:extract && yarn i18n:gen-pages && cd .i18n/worktree && yarn && yarn dev",
    "clean": "rm -rf .next out out-trip out-bus",
    "build:trip": "NEXT_PUBLIC_SITE=trip SITE=trip yarn build && mv out out-trip",
    "build:bus": "next build apps/bus",
    "build:both": "yarn clean && yarn build:trip && yarn build:bus"
  },
